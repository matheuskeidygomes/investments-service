{
  "name": "investments-service",
  "version": "1.0.0",
  "author": "matheuskeidygomes",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.20.2"
  },
  "scripts": {
    "tsc": "rm -rf dist* && tsc",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:ci": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest.config.json",
    "test:k6:score": "k6 run ./tests/load/score-load.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:save": "npx prisma migrate dev --name init",
    "prisma:migrate:run": "npx prisma migrate deploy",
    "prisma:seed:run": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^4.11.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.12.13",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.1.3",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 3000000
  }
}
